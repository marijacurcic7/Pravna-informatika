   "source": [
    "rootdir = 'data'\n",
    "filepaths = []\n",
    "\n",
    "for subdir, dirs, files in os.walk(rootdir):\n",
    "    for file in files:\n",
    "        filename = os.path.join(subdir, file)\n",
    "        filepaths.append(filename)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "977801fc",
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "data = []\n",
    "data_sentences = []\n",
    "for file in filepaths:\n",
    "    f = open(file, \"r\", encoding=\"utf8\")\n",
    "    sadrzaj = f.read()\n",
    "    sadrzaj = sadrzaj.lower()\n",
    "    sadrzaj = sadrzaj.replace(\"\\n\", \" \")\n",
    "    sadrzaj = sadrzaj.replace(\"*\", \"\")\n",
    "    sadrzaj = \" \".join(sadrzaj.split()) #replace multiple whitespace characters with single space\n",
    "    tokens = nltk.sent_tokenize(sadrzaj)\n",
    "    row = [file, tokens]\n",
    "    data.append(row)\n",
    "    for t in tokens:\n",
    "        row_sent = [t, '', file]\n",
    "        data_sentences.append(row_sent)"
   ]
  },
